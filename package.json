{
  "name": "offroad-club-backend",
  "version": "1.0.0",
  "author": "Craig Freeman <craigfreeman@gmail.com>",
  "description": "The backend of offroad club members site",
  "scripts": {
    "start": "node .",
    "restart": "nodemon -e js,graphql -x node .",
    "dev": "NODE_ENV=development nodemon -e js,graphql -x node --inspect .",
    "test": "jest",
    "deploy": "prisma deploy --env-file variables.env",
    "deploy:dev": "prisma deploy --env-file dev.env",
    "deploy:staging": "prisma deploy --env-file staging.env",
    "deploy:prod": "prisma deploy --env-file prod.env"
  },
  "engines": {
    "node": "13.13.0"
  },
  "main": "src/index.js",
  "dependencies": {
    "@sendgrid/mail": "6.5.5",
    "apollo-server-express": "2.22.0",
    "apollo-upload-server": "7.1.0",
    "aws-sdk": "^2.882.0",
    "bcryptjs": "2.4.3",
    "cloudinary": "1.25.1",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "date-fns": "1.30.1",
    "date-fns-timezone": "^0.1.4",
    "dotenv": "6.2.0",
    "express": "4.17.1",
    "graphql": "14.7.0",
    "graphql-binding": "2.5.2",
    "graphql-cli": "3.0.11",
    "graphql-import": "1.0.2",
    "gulp": "4.0.2",
    "jsonwebtoken": "8.3.0",
    "node-fetch": "^2.6.0",
    "nodemailer": "^4.6.8",
    "nodemon": "1.18.4",
    "prisma": "1.34.10",
    "prisma-binding": "2.3.16",
    "stripe": "8.140.0"
  },
  "license": "MIT"
}
