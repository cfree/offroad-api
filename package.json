    "cors": "^2.8.5",
    "cpx": "^1.5.0",
    "date-fns": "^1.29.0",
    "dotenv": "^6.0.0",
    "express": "^4.17.1",
    "glob-fs": "^0.1.7",
    "graphql": "^14.1.1",
    "graphql-binding": "^2.3.5",
    "graphql-cli": "3.0.11",
    "graphql-import": "^1.0.2",
    "gulp": "^4.0.2",
    "jsonwebtoken": "8.3.0",
