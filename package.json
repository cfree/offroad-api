{
  "name": "offroad-club-backend",
  "version": "1.0.0",
  "author": "Craig Freeman <craigfreeman@gmail.com>",
  "description": "The backend of offroad club members site",
  "scripts": {
    "start": "node .",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "prebundle": "webpack --config webpack.config.js",
    "bundle": "cpx 'build/**/*' 'functions'",
    "predev": "NODE_ENV=development webpack --config webpack.config.js",
    "dev": "webpack --watch --config webpack.config.js",
    "test": "jest",
    "deploy": "prisma deploy --env-file variables.env"
  },
  "main": "build/index.js",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@sendgrid/mail": "^6.4.0",
    "apollo-server-express": "^2.13.0",
    "apollo-upload-server": "^7.1.0",
    "bcryptjs": "2.4.3",
    "cloudinary": "^1.17.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "cpx": "^1.5.0",
    "date-fns": "^1.29.0",
    "dotenv": "^6.0.0",
    "express": "^4.17.1",
    "glob-fs": "^0.1.7",
    "graphql": "^14.1.1",
    "graphql-binding": "^2.3.5",
    "graphql-cli": "3.0.11",
    "graphql-import": "^1.0.2",
    "graphql-tag": "^2.10.3",
    "gulp": "^4.0.2",
    "jsonwebtoken": "8.3.0",
    "netlify-lambda": "^1.6.3",
    "node-fetch": "^2.6.0",
    "nodemailer": "^4.6.8",
    "nodemon": "1.18.4",
    "npm-run-all": "^4.1.3",
    "prisma": "^1.34.10",
    "prisma-binding": "^2.3.16",
    "rimraf": "^3.0.2",
    "serverless-http": "^2.3.2",
    "stripe": "^6.12.1",
    "webpack": "^4.43.0",
    "webpack-node-externals": "^1.7.2",
    "webpack-shell-plugin": "^0.5.0"
  },
  "license": "MIT",
  "nodemonConfig": {
    "exec": "npm run start",
    "watch": [
      "build/*.{js,graphql}"
    ]
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "file-loader": "^6.0.0",
    "string-replace-loader": "^2.3.0",
    "ts-loader": "^7.0.4",
    "typescript": "^3.8.3",
    "webpack-cli": "^3.3.11"
  }
}
